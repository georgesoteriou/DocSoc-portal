#bronze.tab-pane.fade(role='tabpanel', aria-labelledby='bronze-tab')
  .container-fluid
    .row.justify-content-center
      .col-lg-8.col-sm-11
        each sponsor in sponsors
          if(sponsor.info.rank === 'Bronze')
            .card.mt-2.mb-2
              .card-body
                .row
                  .col-3
                    img(src="/static/assets/images/sponsors/" + sponsor.info.picture width="150px")
                  .col-7
                    h2.card-title=sponsor.info.name
                    p.card-text=sponsor.info.description
                    a.card-link(href=sponsor.info.link) Read More